<%= form_for @attraction do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :min_height %>
  <%= f.telephone_field :min_height %>
  <%= f.label :happiness_rating %>
  <%= f.telephone_field :happiness_rating %>
  <%= f.label :nausea_rating %>
  <%= f.telephone_field :nausea_rating %>
  <%= f.label :tickets %>
  <%= f.telephone_field :tickets %>
  <%= f.submit %>
<% end %>
